<SCRIPT language=javascript type=text/javascript>
function getCookie(objName) {
	var arrStr = document.cookie.split("; ");
	for ( var i = 0; i < arrStr.length; i++) {
		var temp = arrStr[i].split("=");
		if (temp[0] == objName) return unescape(temp[1]);
	}
}
function re_cookie(value, ext_time, domain) {
	var exp = new Date();
	exp.setTime(exp.getTime() + ext_time * 3600000);
	document.cookie = "tagid=" + value + ";expires=" + exp.toGMTString() + ";domain=" + domain + ";path=/";
}
function sendRequest(pub_id, userid, ext_time, domain) {
	var url_stats = [];
	url_stats.push('http://t.advuser.com/cookie_conn.php?callback=re_cookie&sessionid=');
	url_stats.push(userid);
	url_stats.push('&pid=');
	url_stats.push(pub_id);
	url_stats.push('&time=');
	url_stats.push(ext_time);
	url_stats.push('&domain=');
	url_stats.push(domain);
	var script = document.createElement("script");
	script.type = "text/javascript";
	script.src = url_stats.join('');
	document.body.appendChild(script);
}
function cookieMapping(pub_id, cookie_name, ext_time, delay_time, domain) {
	if (getCookie('tagid')) return;
	setTimeout(function() {
    var userid = getCookie(cookie_name);
	if (!userid) return;
		sendRequest(pub_id, userid, ext_time, domain);
	}, delay_time * 1000);
}
cookieMapping('1', 'sessionid', 87600, 4,'.autohome.com.cn');
</SCRIPT>

